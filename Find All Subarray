vector<ll > arr = {1 , 2 , 3 , 4 , 5};
    for(int i = 0 ; i < arr.size() ; i ++){
        for(int j = i ; j < sz(arr) ; j ++){
            cout << "[ ";
            for(int k = i ; k <= j ; k ++){
                cout << arr[k] << ",";
            }
            cout<<" ]\n";

        }
        cout << endl;
    }
